#!/usr/bin/with-contenv bash

ICAL_ENABLE_HTTP=${ICAL_ENABLE_HTTP:-"TRUE"}
ICAL_ENABLE_HTTPS=${ICAL_ENABLE_HTTPS:-"TRUE"}
ICAL_ENABLE_ICAL_GET=${ICAL_ENABLE_ICAL_GET:-"TRUE"}
ICAL_LISTEN_HOST=${ICAL_LISTEN_HOST:-"*"}
ICAL_LISTEN_HOST_SECURE=${ICAL_LISTEN_HOST_SECURE:-"*"}
ICAL_LISTEN_PORT=${ICAL_LISTEN_PORT:-8080}
ICAL_LISTEN_PORT_SECURE=${ICAL_LISTEN_PORT_SECURE:-8443}
ICAL_SSL_CERT_FILE=${ICAL_SSL_CERT_FILE:-"/certs/core/ical.crt"}
ICAL_SSL_KEY_FILE=${ICAL_SSL_KEY_FILE:-"/certs/core/ical.pem"}
ICAL_SSL_PREFER_SERVER_CIPHERS=${ICAL_SSL_PREFER_SERVER_CIPHERS:-"TRUE"}
